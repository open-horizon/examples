    "react-apollo": "^2.1.9",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.4.1",
    "react-geolocated": "^2.4.0",
    "react-map-gl": "^3.3.4",
    "react-router-dom": "^4.3.1",
    "react-sizeme": "^2.5.2",
    "resolve": "1.6.0",
    "style-loader": "0.19.0",
