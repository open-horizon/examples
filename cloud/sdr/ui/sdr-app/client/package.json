    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "carbon-addons-cloud": "^2.2.0",
    "carbon-addons-cloud-react": "^1.3.2",
    "carbon-components": "^9.18.1",
