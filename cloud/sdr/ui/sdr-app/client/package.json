    "fs-extra": "3.0.1",
    "graphql": "^0.13.2",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "moment": "^2.22.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "query-string": "^5.1.1",
