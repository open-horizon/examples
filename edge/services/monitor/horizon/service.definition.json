{
  "org": "$HZN_ORG_ID",
  "label": "$SERVICE_NAME for $ARCH",
  "url": "$SERVICE_NAME",
  "version": "$SERVICE_VERSION",
  "arch": "$ARCH",
  "public": true,
  "sharable": "singleton",
  "requiredServices": [
    { "org": "$HZN_ORG_ID", "url": "mqtt", "version": "1.0.0", "arch": "$ARCH" }
  ],
  "userInput": [],
  "deployment": {
    "services": {
      "$SERVICE_NAME": {
        "image": "${DOCKER_IMAGE_BASE}_$ARCH:$SERVICE_VERSION",
        "ports": [{"HostPort":"5200:5200:tcp","HostIP":"0.0.0.0"}]
      }
    }
  }
}

